{
  "name": "pcoroneos-nextjs",
  "version": "1.0.0",
  "description": "pcoroneos portfolio using next.js",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "format-check": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "pre-commit": "yarn lint-staged",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "pre-push": "yarn format-check",
    "start": "serve ./out",
    "test": "lerna run test --parallel",
    "typecheck": "lerna run typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulACoroneos/pcoroneos-nextjs.git"
  },
  "author": "paul coroneos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaulACoroneos/pcoroneos-nextjs/issues"
  },
  "homepage": "https://github.com/PaulACoroneos/pcoroneos-nextjs#readme",
  "dependencies": {
    "@types/styled-components": "^4.1.18",
    "next": "^9.0.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.10.0",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": ">=3.2.1 <3.6.0"
  }
}

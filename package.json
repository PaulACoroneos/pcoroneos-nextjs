  },
  "homepage": "https://github.com/PaulACoroneos/pcoroneos-nextjs#readme",
  "dependencies": {
    "@testing-library/jest-dom": "^4.1.2",
    "eslint-plugin-react-hooks": "^2.1.2",
    "jest-styled-components": "^6.3.3",
    "next": "^9.1.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "styled-components": "^4.4.0",
    "typescript": "^3.6.4"
  },
  "devDependencies": {
    "@testing-library/react": "^9.3.0",

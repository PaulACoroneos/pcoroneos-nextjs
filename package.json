{
  "name": "pcoroneos-nextjs",
  "version": "1.0.0",
  "description": "pcoroneos portfolio using next.js",
  "main": "index.js",
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "dev": "next",
    "build": "next build && next export",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "format-check": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "pre-commit": "npm run lint-staged",
    "lint:ts": "eslint . --ext=jsx,ts,tsx src",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "pre-push": "npm run format-check",
    "start": "serve ./out",
    "test": "jest --collect-coverage --passWithNoTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulACoroneos/pcoroneos-nextjs.git"
  },
  "author": "paul coroneos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaulACoroneos/pcoroneos-nextjs/issues"
  },
  "homepage": "https://github.com/PaulACoroneos/pcoroneos-nextjs#readme",
  "dependencies": {
    "global": "^4.4.0",
    "next": "^9.1.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0",
    "styled-components": "^4.4.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^9.1.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@types/styled-components": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cypress": "^3.8.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-cypress": "^2.8.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^9.5.0",
    "npm-check-updates": "^4.0.1",
    "polished": "^3.4.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run --silent lint:ts:fix",
      "npm run test ",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
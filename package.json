{
  "name": "pcoroneos-nextjs",
  "version": "1.0.0",
  "description": "pcoroneos portfolio using next.js",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build && next export",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
    "format-check": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
    "pre-commit": "npm run lint-staged",
    "lint:ts": "eslint . --ext=jsx,ts,tsx src",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "pre-push": "npm run format-check",
    "start": "serve ./out",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulACoroneos/pcoroneos-nextjs.git"
  },
  "author": "paul coroneos",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaulACoroneos/pcoroneos-nextjs/issues"
  },
  "homepage": "https://github.com/PaulACoroneos/pcoroneos-nextjs#readme",
  "dependencies": {
    "global": "^4.4.0",
    "next": "^9.1.4",
    "npm-check-updates": "^3.2.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0",
    "styled-components": "^4.4.1",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/styled-components": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "babel-plugin-polished": "^1.1.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.5",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^10.0.0-1",
    "polished": "^3.4.2",
    "prettier": "^1.19.1",
    "typescript": "^3.7.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run --silent lint:ts:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
